{
  "name": "kb-techspace-notification-server",
  "version": "1.0.0",
  "description": "realtime notification system built on nodejs and socket.io",
  "main": "server.js",
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./",
    "server:prod": "node ./dist",
    "dev": "npm-run-all server",
    "clean": "rimraf dist",
    "build": "babel ./ --out-dir dist",
    "prod": "NODE_ENV=production npm-run-all clean build server:prod",
    "watch": "nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karthikbalajikb/KB-TechSpace-Notification-Server.git"
  },
  "keywords": [
    "nodejs",
    "socket.io",
    "express",
    "realtime"
  ],
  "author": "Karthik Balaji",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/karthikbalajikb/KB-TechSpace-Notification-Server/issues"
  },
  "homepage": "https://github.com/karthikbalajikb/KB-TechSpace-Notification-Server#readme",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-node": "^0.0.1-security",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "morgan-body": "^2.4.8",
    "nodemon": "^2.0.1",
    "npm-run-all": "^4.1.5",
    "socket.io": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-plugin-root-import": "^6.4.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "rimraf": "^3.0.0"
  }
}
